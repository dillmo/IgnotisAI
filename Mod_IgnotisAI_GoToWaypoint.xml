<?xml version="1.0" ?>
<enemy>
    <behaviour>
        <root x="110" y="40">
            <normal>
                <action id="setBool">
                    <string id="id">failed</string>
                    <string id="value" values="flagtoggle">no</string>
                </action>
                <condition id="isButtonDown">
                    <string id="button" values="buttons">FACE_BOTTOM</string>
                    <string id="Comment">stuck fix</string>
                    <normal>
                        <condition id="isOnGround">
                            <string id="condition" values="yesno">yes</string>
                            <string id="check on" values="targetself">self</string>
                            <normal>
                                <condition id="timer">
                                    <string id="timeunit" values="timeunits">seconds</string>
                                    <float id="time">1.00</float>
                                    <string id="execute first time" values="yesno">no</string>
                                    <normal>
                                        <condition id="isButtonDown">
                                            <string id="button" values="buttons">FACE_BOTTOM</string>
                                            <string id="Comment">check again</string>
                                            <normal>
                                                <condition id="isOnGround">
                                                    <string id="condition" values="yesno">yes</string>
                                                    <string id="check on" values="targetself">self</string>
                                                    <normal>
                                                        <action id="pressButton">
                                                            <string id="button" values="buttons">FACE_BOTTOM</string>
                                                            <float id="duration in seconds">0.00</float>
                                                        </action>
                                                    </normal>
                                                </condition>
                                            </normal>
                                        </condition>
                                    </normal>
                                </condition>
                            </normal>
                        </condition>
                    </normal>
                </condition>
                <condition id="isInNamedArea">
                    <string id="area name">JUNGLE</string>
                    <string id="team" values="ownenemy">OWN_TEAM</string>
                    <string id="who" values="targetself">self</string>
                    <string id="Comment">prevent bots from getting stuck in soronas jungle</string>
                    <normal>
                        <condition id="isInNamedArea">
                            <string id="area name">DROP_WARNING</string>
                            <string id="team" values="ownenemy">OWN_TEAM</string>
                            <string id="who" values="targetself">self</string>
                            <else>
                                <condition id="timer">
                                    <string id="timeunit" values="timeunits">seconds</string>
                                    <float id="time">1.00</float>
                                    <string id="execute first time" values="yesno">no</string>
                                    <normal>
                                        <condition id="isInArea">
                                            <string id="condition" values="yesno">yes</string>
                                            <string id="collision groups" values="collison groups" multiselect="true">INVINCIBLE_OBSTACLES;;</string>
                                            <string id="check green obstacles only" values="yesno">no</string>
                                            <string id="teams" values="teams" multiselect="true">OWN_TEAM;;ENEMY_TEAM;;NEUTRAL_TEAM;;</string>
                                            <string id="areaName"></string>
                                            <float id="xOffset">0.04</float>
                                            <float id="yOffset">0.00</float>
                                            <float id="width">0.16</float>
                                            <float id="height">0.05</float>
                                            <string id="check line of sight" values="yesno">no</string>
                                            <string id="ignore invisibility" values="yesno">no</string>
                                            <string id="Comment">check for walls</string>
                                            <normal>
                                                <condition id="isOnGround">
                                                    <string id="condition" values="yesno">yes</string>
                                                    <string id="check on" values="targetself">self</string>
                                                    <normal>
                                                        <condition id="isButtonDown">
                                                            <string id="button" values="buttons">FACE_BOTTOM</string>
                                                            <else>
                                                                <action id="pressButton">
                                                                    <string id="button" values="buttons">FACE_BOTTOM</string>
                                                                    <float id="duration in seconds">0.30</float>
                                                                    <string id="Comment">jump</string>
                                                                </action>
                                                            </else>
                                                        </condition>
                                                    </normal>
                                                </condition>
                                            </normal>
                                        </condition>
                                    </normal>
                                </condition>
                            </else>
                        </condition>
                    </normal>
                </condition>
                <condition id="isNextWaypointPosition">
                    <string id="position" values="offsetposition">BACK</string>
                    <normal>
                        <action id="pressStick">
                            <string id="direction" values="directions">BACKWARD</string>
                            <float id="duration in seconds">0.10</float>
                        </action>
                    </normal>
                    <else>
                        <condition id="isNextWaypointPosition">
                            <string id="position" values="offsetposition">FRONT</string>
                            <normal>
                                <action id="pressStick">
                                    <string id="direction" values="directions">FORWARD</string>
                                    <float id="duration in seconds">0.10</float>
                                </action>
                                <orblock>
                                    <normal>
                                        <condition id="isTargetInArea">
                                            <string id="condition" values="yesno">no</string>
                                            <float id="xOffset">0.00</float>
                                            <float id="yOffset">0.00</float>
                                            <float id="width">0.32</float>
                                            <float id="height">0.32</float>
                                            <string id="check line of sight" values="yesno">no</string>
                                            <string id="ignore invisibility" values="yesno">no</string>
                                            <normal>
                                                <condition id="isNextWaypointPosition">
                                                    <string id="position" values="offsetposition">BELOW</string>
                                                    <else>
                                                        <action id="setBool">
                                                            <string id="id">jump</string>
                                                            <string id="value" values="flagtoggle">yes</string>
                                                        </action>
                                                    </else>
                                                </condition>
                                            </normal>
                                        </condition>
                                    </normal>
                                    <or>
                                        <condition id="isInArea">
                                            <string id="condition" values="yesno">yes</string>
                                            <string id="collision groups" values="collison groups" multiselect="true">PLAYERS;;CREEPS;;</string>
                                            <string id="check green obstacles only" values="yesno">no</string>
                                            <string id="teams" values="teams" multiselect="true">ENEMY_TEAM;;NEUTRAL_TEAM;;</string>
                                            <string id="areaName"></string>
                                            <float id="xOffset">0.00</float>
                                            <float id="yOffset">0.00</float>
                                            <float id="width">0.16</float>
                                            <float id="height">0.02</float>
                                            <string id="check line of sight" values="yesno">no</string>
                                            <string id="ignore invisibility" values="yesno">no</string>
                                            <string id="Comment">Check for enemies and neutrals</string>
                                        </condition>
                                        <condition id="isInArea">
                                            <string id="condition" values="yesno">yes</string>
                                            <string id="collision groups" values="collison groups" multiselect="true">INVINCIBLE_OBSTACLES;;DESTROYABLE_OBSTACLES;;</string>
                                            <string id="check green obstacles only" values="yesno">no</string>
                                            <string id="teams" values="teams" multiselect="true">OWN_TEAM;;ENEMY_TEAM;;NEUTRAL_TEAM;;</string>
                                            <string id="areaName"></string>
                                            <float id="xOffset">0.04</float>
                                            <float id="yOffset">0.00</float>
                                            <float id="width">0.08</float>
                                            <float id="height">0.05</float>
                                            <string id="check line of sight" values="yesno">no</string>
                                            <string id="ignore invisibility" values="yesno">no</string>
                                            <string id="Comment">Check for obstacles</string>
                                        </condition>
                                    </or>
                                </orblock>
                            </normal>
                        </condition>
                        <condition id="isNextWaypointPosition">
                            <string id="position" values="offsetposition">BELOW</string>
                            <normal>
                                <andblock>
                                    <normal>
                                        <action id="enableUpgrade">
                                            <string id="condition" values="yesno">no</string>
                                            <string id="upgrade name">JetterJet</string>
                                        </action>
                                    </normal>
                                    <or>
                                        <condition id="checkClass">
                                            <string id="check on" values="targetself">self</string>
                                            <string id="class">Jetter</string>
                                        </condition>
                                        <condition id="isUpgradeEnabled">
                                            <string id="condition" values="yesno">yes</string>
                                            <string id="upgrade name">JetterJet</string>
                                        </condition>
                                    </or>
                                </andblock>
                                <action id="pressButton">
                                    <string id="button" values="buttons">SHOULDER_LEFT</string>
                                    <float id="duration in seconds">0.10</float>
                                </action>
                            </normal>
                            <else>
                                <condition id="isNextWaypointPosition">
                                    <string id="position" values="offsetposition">ABOVE</string>
                                    <normal>
                                        <action id="setBool">
                                            <string id="id">jump</string>
                                            <string id="value" values="flagtoggle">yes</string>
                                        </action>
                                    </normal>
                                    <else>
                                        <orblock>
                                            <string id="Comment">I can&apos;t reach my waypoint</string>
                                            <else>
                                                <action id="setBool">
                                                    <string id="id">failed</string>
                                                    <string id="value" values="flagtoggle">yes</string>
                                                </action>
                                            </else>
                                            <or>
                                                <condition id="isNextWaypointPosition">
                                                    <string id="position" values="offsetposition">FRONT</string>
                                                </condition>
                                                <condition id="isAtDestWaypoint" />
                                            </or>
                                        </orblock>
                                    </else>
                                </condition>
                            </else>
                        </condition>
                    </else>
                </condition>
                <action id="executeBehaviourTree">
                    <string id="fileName">Mod_IgnotisAI_Jump</string>
                    <string id="Minimized">yes</string>
                </action>
            </normal>
        </root>
    </behaviour>
</enemy>
