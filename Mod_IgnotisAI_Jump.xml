<?xml version="1.0" ?>
<enemy>
    <behaviour>
        <root x="110" y="40">
            <string id="Comment">Handle jumping here to prevent getting stuck</string>
            <normal>
                <condition id="getBoolEquals">
                    <string id="id">jump</string>
                    <string id="value" values="yesno">yes</string>
                    <normal>
                        <action id="setBool">
                            <string id="id">jump</string>
                            <string id="value" values="flagtoggle">no</string>
                        </action>
                        <orblock>
                            <normal>
                                <condition id="timer">
                                    <string id="timeunit" values="timeunits">seconds</string>
                                    <float id="time">0.19</float>
                                    <string id="execute first time" values="yesno">yes</string>
                                    <else>
                                        <action id="pressButton">
                                            <string id="button" values="buttons">FACE_BOTTOM</string>
                                            <float id="duration in seconds">0.10</float>
                                        </action>
                                    </else>
                                </condition>
                            </normal>
                            <else>
                                <orblock>
                                    <normal>
                                        <condition id="timer">
                                            <string id="timeunit" values="timeunits">seconds</string>
                                            <float id="time">0.39</float>
                                            <string id="execute first time" values="yesno">yes</string>
                                            <else>
                                                <action id="pressButton">
                                                    <string id="button" values="buttons">FACE_BOTTOM</string>
                                                    <float id="duration in seconds">0.10</float>
                                                </action>
                                            </else>
                                        </condition>
                                    </normal>
                                    <else>
                                        <orblock>
                                            <normal>
                                                <condition id="timer">
                                                    <string id="timeunit" values="timeunits">seconds</string>
                                                    <float id="time">0.79</float>
                                                    <string id="execute first time" values="yesno">yes</string>
                                                    <else>
                                                        <action id="pressButton">
                                                            <string id="button" values="buttons">FACE_BOTTOM</string>
                                                            <float id="duration in seconds">0.10</float>
                                                        </action>
                                                    </else>
                                                </condition>
                                            </normal>
                                            <else>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Jetter</string>
                                                    <normal>
                                                        <condition id="isUpgradeEnabled">
                                                            <string id="condition" values="yesno">yes</string>
                                                            <string id="upgrade name">JetterJet</string>
                                                            <normal>
                                                                <action id="pressStick">
                                                                    <string id="direction" values="directions">UP</string>
                                                                    <float id="duration in seconds">0.10</float>
                                                                </action>
                                                            </normal>
                                                            <else>
                                                                <action id="enableUpgrade">
                                                                    <string id="condition" values="yesno">yes</string>
                                                                    <string id="upgrade name">JetterJet</string>
                                                                </action>
                                                                <action id="pressButton">
                                                                    <string id="button" values="buttons">FACE_BOTTOM</string>
                                                                    <float id="duration in seconds">0.10</float>
                                                                </action>
                                                            </else>
                                                        </condition>
                                                    </normal>
                                                    <else>
                                                        <action id="setBool">
                                                            <string id="id">failed</string>
                                                            <string id="value" values="flagtoggle">yes</string>
                                                        </action>
                                                    </else>
                                                </condition>
                                            </else>
                                            <or>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Dasher</string>
                                                </condition>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Chameleon</string>
                                                </condition>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Paladin</string>
                                                </condition>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Maw</string>
                                                </condition>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Hunter</string>
                                                </condition>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Vampire</string>
                                                </condition>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Summoner</string>
                                                </condition>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Blazer</string>
                                                </condition>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Captain</string>
                                                </condition>
                                                <condition id="checkClass">
                                                    <string id="check on" values="targetself">self</string>
                                                    <string id="class">Shaman</string>
                                                </condition>
                                            </or>
                                        </orblock>
                                    </else>
                                    <or>
                                        <condition id="checkClass">
                                            <string id="check on" values="targetself">self</string>
                                            <string id="class">Cowboy</string>
                                        </condition>
                                        <condition id="checkClass">
                                            <string id="check on" values="targetself">self</string>
                                            <string id="class">Brute</string>
                                        </condition>
                                        <condition id="checkClass">
                                            <string id="check on" values="targetself">self</string>
                                            <string id="class">Commando</string>
                                        </condition>
                                        <condition id="checkClass">
                                            <string id="check on" values="targetself">self</string>
                                            <string id="class">Assassin</string>
                                        </condition>
                                        <condition id="checkClass">
                                            <string id="check on" values="targetself">self</string>
                                            <string id="class">Blinker</string>
                                        </condition>
                                    </or>
                                </orblock>
                            </else>
                            <or>
                                <condition id="checkClass">
                                    <string id="check on" values="targetself">self</string>
                                    <string id="class">Tank</string>
                                </condition>
                                <condition id="checkClass">
                                    <string id="check on" values="targetself">self</string>
                                    <string id="class">Heavy</string>
                                </condition>
                                <condition id="checkClass">
                                    <string id="check on" values="targetself">self</string>
                                    <string id="class">Bird</string>
                                </condition>
                                <condition id="checkClass">
                                    <string id="check on" values="targetself">self</string>
                                    <string id="class">Butterfly</string>
                                </condition>
                                <condition id="checkClass">
                                    <string id="check on" values="targetself">self</string>
                                    <string id="class">Spy</string>
                                </condition>
                            </or>
                        </orblock>
                    </normal>
                </condition>
            </normal>
        </root>
    </behaviour>
</enemy>
